VITE_AMAP_KEY=YOUR_AMAP_KEY
AMAP_WEB_KEY=YOUR_AMAP_KEY
# 可选：如果你的 Key 开启了「JSAPI 安全密钥」，需要同时配置
# VITE_AMAP_SECURITY_CODE=YOUR_SECURITY_JS_CODE
# 可选：前端访问密码（取消注释后生效）
# functionlock=YOUR_PASSWORD
# 可选：密码有效时间（小时，允许小数）。仅在 functionlock 启用时生效；0/留空/注释代表每次都要输入
functionlock_hours=12

# 后端端口（vite dev proxy 默认指向 5174）
PORT=5174

# WebService 限流（避免 10021：CUQPS_HAS_EXCEEDED_THE_LIMIT）
AMAP_QPS=3
AMAP_CONCURRENCY=2
AMAP_MAX_RETRIES=3
# 单次请求超时（毫秒）
AMAP_TIMEOUT_MS=8000
# 限流等待队列长度（超过直接拒绝）
AMAP_QUEUE_LIMIT=120
